<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape android:shape="oval">
            <solid android:color="?colorPrimaryBase" />
            <size android:height="12.0dip" android:width="12.0dip" />
        </shape>
    </item>
    <item>
        <layer-list>
            <item>
                <shape android:shape="oval">
                    <solid android:color="?colorSurface" />
                    <size android:height="12.0dip" android:width="12.0dip" />
                </shape>
            </item>
            <item>
                <inset android:inset="1.5dip">
                    <shape android:shape="oval">
                        <solid android:color="?colorContentDisabled" />
                    </shape>
                </inset>
            </item>
        </layer-list>
    </item>
</selector>