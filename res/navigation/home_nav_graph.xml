    </action>
    <action android:id="@id/navigateToLowBatteriesFragment" app:destination="@id/low_batteries_fragment" app:enterAnim="@anim/enter_from_right" app:exitAnim="@anim/exit_to_left" app:popEnterAnim="@anim/enter_from_left" app:popExitAnim="@anim/exit_to_right" />
    <fragment android:name="com.immediasemi.blink.settings.subscription.plus.PlusPlanBenefitsUpsellFragment" android:id="@id/plus_plan_benefits_upsell_fragment">
        <argument android:name="messagePriority" android:defaultValue="0L" app:argType="long" />
    </fragment>
    <fragment android:label="@string/camera_battery_low" android:name="com.immediasemi.blink.home.system.LowBatteriesFragment" android:id="@id/low_batteries_fragment">
        <argument android:name="deviceName" app:argType="string" />
        <argument android:name="genericDeviceNameRes" app:argType="integer" />
        <argument android:name="genericDeviceNamePluralRes" app:argType="integer" />
    </fragment>
</navigation>