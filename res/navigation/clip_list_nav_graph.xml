    </fragment>
    <fragment android:label="moments_tutorial_details_fragment" android:name="com.immediasemi.blink.video.clip.moment.tutorial.MomentsTutorialDetailsFragment" android:id="@id/moments_tutorial_details_fragment" />
    <fragment android:name="com.immediasemi.blink.settings.subscription.plus.PlusPlanBenefitsUpsellFragment" android:id="@id/plus_plan_benefits_upsell_fragment">
        <argument android:name="messagePriority" android:defaultValue="0L" app:argType="long" />
    </fragment>
    <fragment android:name="com.immediasemi.blink.common.subscription.SubscriptionUpgradeFragment" android:id="@id/subscription_upgrade_fragment" />
