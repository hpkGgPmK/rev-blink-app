<?xml version="1.0" encoding="utf-8"?>
<navigation android:id="@id/clip_list_nav_graph" app:startDestination="@id/clip_list"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <action android:id="@id/navigateToCLipListFragment" app:destination="@id/clip_list" app:popUpTo="@id/clip_list" app:popUpToInclusive="true" />
